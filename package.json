{
    "name": "pubg-buddy",
    "version": "1.0.0",
    "description": "A bot that reports PUBG stats",
    "main": "src/index.ts",
    "author": "Thomas Ortiz",
    "scripts": {
        "build:dev": "webpack --config webpack.config.dev.js",
        "build:prod": "webpack --config webpack.config.prod.js",
        "migrate": "node ./dist/migrate.bundle.js",
        "start": "node --optimize_for_size --max_old_space_size=460 dist/index.bundle.js",
        "sql": "heroku pg:psql -a pubg-stat-discord-bot",
        "sql-test": "heroku pg:psql -a pubg-stat-discord-bot-dev"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "dblapi.js": "^2.3.0",
        "discord.js": "^11.4.2",
        "dotenv": "^6.1.0",
        "jimp": "^0.5.6",
        "mixpanel": "^0.9.2",
        "node-cache": "^4.2.0",
        "pg": "^7.6.1",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@types/mixpanel": "^2.14.0",
        "@types/node": "^10.12.5",
        "@types/pg": "^7.4.11",
        "clean-webpack-plugin": "^1.0.0",
        "eslint": "^5.9.0",
        "progress-bar-webpack-plugin": "^1.11.0",
        "speed-measure-webpack-plugin": "^1.2.5",
        "ts-loader": "^5.3.3",
        "typescript": "^3.1.6",
        "webpack": "^4.28.4",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.2.1",
        "webpack-merge": "^4.2.1",
        "webpack-node-externals": "^1.7.2"
    }
}
